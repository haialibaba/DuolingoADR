<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingConstraints"
    android:orientation="vertical"
    android:paddingHorizontal="10dp"
    android:background="@drawable/gradient_background_startclass">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        >
        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/icon_close"
            />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerInParent="true"
            android:background="@drawable/gradient_background_premium"
            android:padding="5dp"
            android:text="PREMIUM"
            android:textColor="#FFFFFFFF"
            android:textSize="18sp" />
    </RelativeLayout>

    <TextView
        android:id="@+id/textView16"
        android:layout_width="274dp"
        android:layout_height="74dp"
        android:text="Choose a plan for after your 14-day free trial"
        android:textColor="#FFFFFF"
        android:textFontWeight="600"
        android:textSize="24sp"
        android:textAlignment="center"
        android:layout_gravity="center"
        android:layout_marginTop="90dp"
        />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_gravity="center"
        android:layout_marginTop="20dp" >

        <TextView
            android:id="@+id/textView25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@id/relativelayoutFamilyPlan"
            android:background="@drawable/icon_check"
            android:translationZ="1dp" />

        <RelativeLayout
            android:id="@+id/relativelayoutFamilyPlan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            >

            <TextView
                android:id="@+id/textView20"
                android:layout_width="331dp"
                android:layout_height="107dp"
                android:background="@drawable/custom_textview_border"
                android:textColor="#3F51B5"
                android:textFontWeight="600"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="wrap_content"
                android:layout_height="26dp"
                android:background="@drawable/custom_tag_radius"
                android:text="2-6 MEMBERS"
                android:textColor="#FFFFFF"
                android:textSize="16sp"
                android:paddingHorizontal="10dp"/>

            <TextView
                android:id="@+id/textView22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Family Plan"
                android:textColor="#5E71DC"
                android:textFontWeight="600"
                android:textSize="22sp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"/>

            <TextView
                android:id="@+id/textView23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="12 mo $119.99"
                android:textColor="#5E71DC"
                android:textFontWeight="600"
                android:textSize="16sp"
                android:layout_marginLeft="20dp"
                android:layout_alignBottom="@id/textView20"
                android:layout_marginBottom="20dp"
                />

            <TextView
                android:id="@+id/textView24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="$9.99 / MO"
                android:textColor="#5E71DC"
                android:textFontWeight="600"
                android:textSize="16sp"
                android:layout_marginLeft="20dp"
                android:layout_alignEnd="@id/textView20"
                android:layout_centerVertical="true"
                android:layout_marginRight="20dp"/>
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_gravity="center"
        android:layout_marginTop="5dp" >

        <TextView
            android:id="@+id/textView26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/icon_check"
            android:layout_alignEnd="@id/relativelayoutIndividual"
            android:translationZ="1dp"
            />

        <RelativeLayout
            android:id="@+id/relativelayoutIndividual"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            >

            <TextView
                android:id="@+id/textView27"
                android:layout_width="331dp"
                android:layout_height="107dp"
                android:background="@drawable/custom_textview_border"
                android:textColor="#3F51B5"
                android:textFontWeight="600"
                android:textSize="20sp"
                />

            <TextView
                android:id="@+id/textView28"
                android:layout_width="wrap_content"
                android:layout_height="26dp"
                android:background="@drawable/custom_tag_radius"
                android:text="MOST POPULAR"
                android:textColor="#FFFFFF"
                android:textSize="16sp"
                android:paddingHorizontal="10dp"/>

            <TextView
                android:id="@+id/textView29"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Individual"
                android:textColor="#5E71DC"
                android:textFontWeight="600"
                android:textSize="22sp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"/>

            <TextView
                android:id="@+id/textView30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="12 mo $83.99"
                android:textColor="#5E71DC"
                android:textFontWeight="600"
                android:textSize="16sp"
                android:layout_marginLeft="20dp"
                android:layout_alignBottom="@id/textView27"
                android:layout_marginBottom="20dp"
                />

            <TextView
                android:id="@+id/textView31"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="$6.99 / MO"
                android:textColor="#5E71DC"
                android:textFontWeight="600"
                android:textSize="16sp"
                android:layout_marginLeft="20dp"
                android:layout_alignEnd="@id/textView27"
                android:layout_centerVertical="true"
                android:layout_marginRight="20dp"/>
        </RelativeLayout>
    </RelativeLayout>

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cancel anytime in the App Store"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textAlignment="center"
        android:layout_gravity="center"
        android:layout_marginTop="60dp"
        />

    <Button
        android:id="@+id/btnStartMyFree2Weeks"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/custom_radius_white"
        android:hapticFeedbackEnabled="false"
        android:text="START MY FREE 2 WEEKS"
        android:gravity="center"
        android:layout_marginTop="10dp"
        android:textStyle="bold"
        android:layout_gravity="center"
        android:textSize="16sp"
        />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="VIEW ALL PLANS"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textAlignment="center"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:textStyle="bold"
        />

</LinearLayout>